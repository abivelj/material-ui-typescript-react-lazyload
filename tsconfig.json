{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "experimentalDecorators": true,
    "lib": [ "dom", "es2015", "es2015.promise", "esnext", "webworker", "esnext", "scripthost"]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
